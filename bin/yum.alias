#!/usr/bin/env bash

# yum.alias (loaded in bashrc)

alias yum-erase='sudo yum erase '
alias yum-group-install='sudo yum group install '
alias yum-group-list='sudo yum group list '
alias yum-install='sudo yum install '
alias yum-list='sudo yum list '
alias yum-uninstall='yum-erase '
alias yum-update='sudo yum update '
alias yum-upgrade='sudo yum upgrade '
