#!/usr/bin/env bash

echo "$(date): git-help" >> "$HOME/.gitaudit"

# git-help

declare tmpfile
tmpfile=$(mktemp -t tmp.git-help.XXXXXXXXXX)
# shellcheck disable=SC2012
ls -c1 "$PERSONALBIN"/*git* 2>/dev/null| sed 's|.*/|script |' > "${tmpfile}".gh2
declare -F | grep git | grep -v _git > "${tmpfile}".gh3

sort -u "${tmpfile}".gh*
\rm -f "${tmpfile}".gh*
