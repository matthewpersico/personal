#!/usr/bin/env bash

echo "$(date): git-help" >> "$HOME/.gitaudit"

# git-help

use mktemp
use on_exit

declare gh2
gh2=$(mktempfile +gh2)
rm-on-exit "$gh2"

declare gh3
gh3=$(mktempfile +gh3)
rm-on-exit "$gh3"

find "$PERSONALBIN" -name \*git\* -executable | grep /git | sed 's|.*/|script |' > "${gh2}"
declare -F | grep git | grep -v _git > "${gh3}"

sort -u "${gh2}" "${gh3}"
