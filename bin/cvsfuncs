# -*- sh -*-

# cvsfuncs

CVSFUNCS_FILE=${BASH_SOURCE[0]}

auditfuncs_init $CVSFUNCS_FILE

cvsfuncs-edit ()
{
    cvsfuncs_audit "$@"
    xo $CVSFUNCS_FILE
}

cvsfuncs-reload ()
{
    cvsfuncs_audit "$@"
    . $CVSFUNCS_FILE
}
alias cvsfuncs-load=cvsfuncs-reload

cvsfuncs-help () {
    help_strings $CVSFUNCS_FILE "$@"
}

alias co='echo No CVS usage allowed here. Typo? #'
