#!/usr/bin/env bash

# misc.alias (loaded in bashrc)

alias tss='ts "%Y-%m-%dT%H:%M:%.S" '
alias ww='withwhich '
alias wwhich='withwhich '
alias psme='pssearch --U $USER '
alias wz='withzenity '
alias vicrontab='EDITOR=vi crontab -e'
alias sudovicrontab='EDITOR=vi sudo crontab -e'
alias findlocal='echo use "find paths -xdev fileoptions files"'

# If there is an alias, it probably has color junk, so get rid of it.
alias ls &>/dev/null && unalias ls
