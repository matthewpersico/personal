# -*- sh -*-

# autoloadfuncs

auditfuncs-init
## We use this contruction to find controlfuncs_build because we load this
## before we set up personal path stuff.
. $(dirname $AUTOLOADFUNCS_FILE)/controlfuncs_build

##
## The guts are elsewhere
##
gutsfile=$(dirname $AUTOLOADFUNCS_FILE)/autoload

. $gutsfile

for autoloadfuncsfunc in $(grep -E '^[a-z1-9_]+\(\)' $gutsfile | sed 's/()//')
do
    safe_func_export $autoloadfuncsfunc
done

auditfuncs-end
