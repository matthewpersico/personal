#set -x
log1=$(ls -rt $* | head -1)
log2=$(grep 'build failure, log at:' $log1 | perl -anf -e  'END{print $F[-1]'})
more $log2
echo
echo Remoted to $( nslookup -query=name $(grep 'selected host' $log2 | tail -1 | perl -ape 's/.*\(([0-9.]+)\).*/$1/') | grep 'name =' | perl -ape 's/.*name = //;chop;chop;')
#set +x

