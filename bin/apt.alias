# -*- sh -*-
# shellcheck shell=bash

# apt.alias (loaded in bashrc)

alias apt-install-deps='sudo apt build-dep '
alias apt-install='sudo apt install '
alias apt-remove='sudo apt remove '
alias apt-update='sudo apt update '
alias apt-upgrade='sudo apt upgrade '
alias apt-hold='sudo apt-mark hold '
alias apt-unhold='sudo apt-mark unhold '
