#!/usr/bin/env bash

# dotfilesfuncs-help

declare tmpfile
tmpfile=$(mktemp -t tmp.dotfilesfuncs_help.XXXXXXXXXX)
declare -F | grep dot | sed 's|.* ||' > "${tmpfile}.df1"
declare -F | grep syml | sed 's|.* ||' >> "${tmpfile}.df1"
alias | grep dot | sed 's|.* ||' >> "${tmpfile}.df1"
sort -u "${tmpfile}.df1" | grep -v '^_'
\rm -f "${tmpfile}.df1"
