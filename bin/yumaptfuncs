# -*- sh -*-

# yumaptfuncs

auditfuncs-init
. $(which controlfuncs_build)

apt-build-dep ()
{
    sudo apt-get build-dep "$@"
}
safe_func_export apt-build-dep

apt-install-deps ()
{
    sudo apt-get build-dep "$@"
}
safe_func_export apt-install-deps

apt-install ()
{
    sudo apt-get install "$@"
}
safe_func_export apt-install

apt-remove ()
{
    sudo apt-get remove "$@"
}
safe_func_export apt-remove

apt-update ()
{
    sudo apt-get update "$@"
}
safe_func_export apt-update

apt-upgrade ()
{
    sudo apt-get upgrade "$@"
}
safe_func_export apt-upgrade

aptfuncs-edit ()
{
    yumaptfuncs-edit "$@"
}
safe_func_export aptfuncs-edit

aptfuncs-reload ()
{
    yumaptfuncs_reload "$@"
}
safe_func_export aptfuncs-reload

yum-erase ()
{
    sudo yum erase "$@"
}
safe_func_export yum-erase

yum-group-install ()
{
    sudo yum group install "$@"
}
safe_func_export yum-group-install

yum-group-list ()
{
    sudo yum group list "$@"
}
safe_func_export yum-group-list

yum-install ()
{
    sudo yum install "$@"
}
safe_func_export yum-install

yum-list ()
{
    sudo yum list "$@"
}
safe_func_export yum-list

yum-uninstall ()
{
    yum-erase "$@"
}
safe_func_export yum-uninstall

yum-update ()
{
    sudo yum update "$@"
}
safe_func_export yum-update

yumfuncs-edit ()
{
    yumaptfuncs_edit "$@"
}
safe_func_export yumfuncs-edit

yumfuncs-reload ()
{
    yumaptfuncs_reload "$@"
}
safe_func_export yumfuncs-reload

auditfuncs-end
