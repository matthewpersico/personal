[alias]
    aliases = config --get-regexp alias
    br = branch
    branches = branch
    cdiff = diff --cached
    cleando = clean -df
    cleantest = clean -dn
    co = checkout
    commitv = commit --verbose
    diffc = diff --cached
    doclean = clean -df
    dorealclean = clean -dxf
    grepl = grep -l
    grepn = grep -n
    grepnc = grep --no-color
    grepnp = !git --no-pager grep
    logh = log -1
    logl = log --name-only
    pop = stash pop
    realcleando = clean -dxf
    realcleantest = clean -dxn
    rollback = checkout --
    st = status
    stl = status .
    stnc = status --no-color
    stnou = status -uno
    testclean = clean -dn
    testrealclean = clean -dxn
    uncheck = checkout --
    uncheckout = checkout --
    unstage = reset HEAD --

[push]
    default = simple
; Uncomment the following to force all pushes to require repo and branch args.
; [remote]
;     pushdefault = PUSHREMOTE_NOT_CONFIGURED

[merge]
    conflictstyle = diff3

[color]
    branch = auto

;; Color info...
;; https://git-scm.com/docs/git-config
;; http://www.calmar.ws/vim/256-xterm-24bit-rgb-color-chart.html
;; http://vim.wikia.com/wiki/Xterm256_color_names_for_console_Vim
;; /usr/share/X11/rgb.txt (find /usr -name rgb.txt)

[core]
    pager = less -RF
    editor = emacsclient -c

[include]
    path = .gitconfig.personal
    path = .gitconfig.bloomberg
[color "diff"]
	frag = cyan
	new = green
[credential]
	helper = cache --timeout=2147483648
