# -*- sh -*-
# shellcheck shell=sh

# <Function Class: util>
# <Function Justification: none>

tail_number_strip ()
{
    ##@@ none||none||tail_number_strip||From each argument (in theory the product of tail_number_add), strip from the '(' to the end.
    local a
    for a in "$@"
    do
        echo "${a%(*}"
    done
    return $?
}
