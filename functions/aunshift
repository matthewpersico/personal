# -*- sh -*-

# <Function Class: bash arrays>
# <Function Justification: env mod>

aunshift ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||apush||Prepend arg2, arg3, etc.. to the beginning of the array named in arg1.

    local array_name="$1";shift
    asplice $array_name 0 0 "$@"
    return $?
}
