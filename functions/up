#!/usr/bin/env bash

# <Function Class: cd>
# <Function Justification: change cwd>

up ()
{
    local tgt=$1
    if [ -z "$tgt" ]; then
        tgt=1
    fi

	if is-int "$tgt"; then
        if is-neg "$tgt"; then
            echo 'up takes positive numbers. We will convert for you.' >&2
        else
            ((tgt=tgt*-1))
        fi
        dirlist $tgt
    else
        echo 'up takes positive numbers.' >&2
    fi
}
