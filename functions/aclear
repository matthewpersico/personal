aclear ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||aclear||Remove all the elements from the array named in arg1.

    declare -n array_alias="$1";shift
    array_alias=()
    return $(true)
}
