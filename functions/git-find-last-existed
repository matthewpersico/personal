# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-find-last-existed ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-find-last-existed||The revision before which the given file was deleted.

    git rev-list -n 1 $(git-find-deleted "$1")^ -- "$1"
}
