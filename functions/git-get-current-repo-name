# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-get-current-repo-name ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-get-current-repo-name||

    git remote -v | grep origin | sed 's|.*/||' | sed 's/ .*//' | sort -u
}
