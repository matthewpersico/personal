# -*- sh -*-

# <Function Class: git

git-parse-remote-v ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-parse-remote-v||

    gitfuncs_remotes=()
    for i in $( git remote -v 2>/dev/null | sed -r -e 's/\(.*//' -e 's/ +$//' -e 's/\t/=/' | sort -ru)
    do
        key=${i%=*}
        value=${i#*=}
        gitfuncs_remotes[$key]=$value
    done
}


