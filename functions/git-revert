# -*- sh -*-

# <Function Class: git

git-revert ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-revert||revert to the latest unmodified version on the current branch, losing any modifications.
    git checkout -- "$@"
}


