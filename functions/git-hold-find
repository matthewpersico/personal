# -*- sh -*-

# <Function Class: git

git-hold-find ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-hold-find||show all entries in the git hold stash
    find $(git-root)/.githold -type f "$@" 2>/dev/null
}
