# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-what-is-edited ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-edited||

    if [ -n "$1" ]
    then
        echo 'Files altered: staged, unstaged and untracked'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --all-mod
    fi
}
