# -*- sh -*-

# <Function Class: git

git-what-is-unstaged ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-unstaged||

    if [ -n "$1" ]
    then
        echo 'Files altered, not staged for commit'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --unstaged
    fi
}


