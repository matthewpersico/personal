# -*- sh -*-

# <Class: bash arrays>
# <Function Justification: env mod>

ashift ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||ashift||Remove one value from the beginning of the array named in arg1 and print it.

    local array_name="$1";shift
    asplice --removed $array_name 0 1
    return $?
}
