# -*- sh -*-

# <Class: bash arrays>
# <Function Justification: env mod>

apop ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||apop||Remove one value from the end of the array named in arg1 and print it.

    local array_name="$1";shift
    asplice --removed $array_name -1
    return $?
}
