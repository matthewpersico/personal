# -*- sh -*-

# <Function Class: hist>
# <Function Justification: none>

rename_history ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    if [ -z "$1" ]
    then
        echo usage: rename_history suffix
    else
        cp $HISTFILE "${HISTFILE}.$1"
        HISTFILE="${HISTFILE}.$1"
    fi
    echo "Done"
}
