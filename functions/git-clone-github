# -*- sh -*-

# <Function Class: gitfuncs.github>
# <Function Justification: change cwd>

git-clone-github ()
{
    ##@@ none||none||github-clone||

    local usage_func="git-clone-usage"
    [ -z "$1" ] && $usage_func ${FUNCNAME} github && return $NOT_OK

    git-clone --proto github "$@"
    return $?
}
