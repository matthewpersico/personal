# -*- sh -*-

# <Function Class: cd>
# <Function Justification: env mod>

prunedirlist ()
{
    ## Remove all the intermediate directories
	cleandirlist
    local tmplist
    tmplist=$(echo "${DIRLIST}" | tr ' ' '\n' | sort -ur)
    local lastdir
    local newlist
    local i
    for i in $tmplist
    do
        if [[ "${lastdir}" != "${i}"* ]]
        then
            newlist="$newlist $i"
        fi
        lastdir=$i
    done
    DIRLIST="$newlist"
    setdirlist
}
