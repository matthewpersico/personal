# -*- sh -*-

# <Function Class: cd>

prunedirlist ()
{
    ## Remove all the intermediate directories
	cleandirlist
    local tmplist=$(echo ${DIRLIST} | tr ' ' '\n' | sort -ur)
    local lastdir
    local newlist
    local i
    for i in $tmplist
    do
        if [[ "${lastdir}" != "${i}"* ]]
        then
            newlist="$newlist $i"
        fi
        lastdir=$i
    done
    DIRLIST="$newlist"
    setdirlist
}


