# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-hold-list ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-hold-list||show all entries in the git hold stash
    local ghf=$(git-hold-find)
    [ -n "$ghf" ] && ls -la $(ghf) | sed "s|/.*.githold/||"
}
