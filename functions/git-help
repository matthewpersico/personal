# -*- sh -*-

# <Function Class: git

git-help ()
{
    ##@@ 0001||group||git-help||

    local tmpfile=$(mktemp --suffix=.git-help)
    ls -c1 $PERSONALBIN/*git* 2>/dev/null| sed 's|.*/|script |' > ${tmpfile}.gh2
    declare -F | grep git | grep -v _git > ${tmpfile}.gh3

    sort -u ${tmpfile}.gh*
    \rm -f ${tmpfile}.gh*
}


