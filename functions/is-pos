# -*- sh -*-

# <Function Class: util>
# <Function Justification: none>

is-pos ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||is-pos||Is an integer and is >= 0. Return 0 if true.

    is-neg $1 && return $(false)
    return $(true)
}
