# -*- sh -*-

# <Function Class: X>
# <Function Justification: none>

x-current-root-guts ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    x-is-available;
    if [ ! "$?" = "${TRUE}" ]
    then
        return $FALSE
    fi
    echo $(xwininfo -root | grep -i "$1:" | sed 's/.* //')
    return $TRUE
}
