# -*- sh -*-

# <Function Class: git

git-what-is-staged ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-staged||

    if [ -n "$1" ]
    then
        echo 'Files altered, staged for commit'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --staged
    fi
}


