#!/usr/bin/env bash

# <Function Class: util>
# <Function Justification: env access>

func-name-spoof ()
{

    local spoofing=1
    local iam=${FUNCNAME[1]}
    if [ -n "${FUNCNAME[2]}" ] && \
           [ ! "${FUNCNAME[2]}" = 'source' ] && \
           [ ! "${FUNCNAME[2]}" = 'main' ]
    then
        iam="${FUNCNAME[2]}"
        spoofing=0
    fi
    echo "$iam"
    return $spoofing
}
