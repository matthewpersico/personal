# -*- sh -*-

# <Function Class: util>

func-name-spoof ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit

    local spoofing=$(false)
    local iam=${FUNCNAME[1]}
    if [ -n "${FUNCNAME[2]}" ] && \
           [ ! "${FUNCNAME[2]}" = 'source' ] && \
           [ ! "${FUNCNAME[2]}" = 'main' ]
    then
        iam="${FUNCNAME[2]}"
        spoofing=$(true)
    fi
    echo $iam
    return $spoofing
}


