# -*- sh -*-

# <Function Class: util>
# <Function Justification: none>

is-int ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||is-int||Is an integer. Return 0 if true.

    if [[ $1 =~ ^-?[0-9]+$ ]]
    then
        return $(true)
    else
        return $(false)
    fi

}
