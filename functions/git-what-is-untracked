# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-what-is-untracked ()
{
    ##@@ 0001||group||git-what-is-untracked||

    if [ -n "$1" ]
    then
        echo 'Files not tracked in the repo'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --untracked
    fi
}
