# -*- sh -*-

# <Function Class: util>

list ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||list||Print each argument on its own line.

    ## Don't use echo "$@" | tr ' ' '\n'; it will not respect spaces inside of
    ## an argument.
    for i in "$@"
    do
        echo $i
    done
    return $(true)
}


