# -*- sh -*-

# <Function Class: util>
# <Function Justification: none>

list ()
{
    ##@@ none||none||list||Print each argument on its own line.

    ## Don't use echo "$@" | tr ' ' '\n'; it will not respect spaces inside of
    ## an argument.
    for i in "$@"
    do
        echo "$i"
    done
    return "$OK"
}
