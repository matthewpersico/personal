# -*- sh -*-

# <Function Class: git

git-cat-cl ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-cat-cl||'cat' the ChangeLog in the git root

    local cl=$(git-root)/$GITFUNCS_CHANGELOG_NAME
    [ -r $cl ] && cat $cl && return $OK
    echo "$cl not found."
    return $NOT_OK
}


