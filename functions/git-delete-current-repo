# -*- sh -*-

# <Function Class: git

git-delete-current-repo ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-delete-current-repo||

    declare -A gitfuncs_remotes
    git-parse-remote-v
    git-delete-repo $(echo ${gitfuncs_remotes[origin]} | tr '[/:]' '[  ]')
    return $?
}


