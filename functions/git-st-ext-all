# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-st-ext-all ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-st-ext||Read all notes about repo current statuses

    for i in $(git-go --listfull)
    do
        ( builtin cd $i
          local s=$(git-st-ext)
          [ -n "$s" ] && echo $i - $s
        )
    done
}
