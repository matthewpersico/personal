# -*- sh -*-

# <Function Class: cd>
# <Function Justification: env mod>

cleandirlist ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ## Remove the current dir marker
    [ -n "$DIRLIST" ] && \
	    DIRLIST=$(echo "${DIRLIST}" | sed "s|$DIRLIST_PWD_INDIC||g")
}
