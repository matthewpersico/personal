# -*- sh -*-

# <Function Class: util>

aset ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||asplit||Set all the elements in the array named in arg1 to the values of arg2, args3, ...

    declare -n array_alias="$1";shift;
    array_alias=("$@")
    return $(true)
}


