# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-worktree-diff-from-root ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-worktree-diff-from-root||diff root file against worktree file

    git-worktree-diff-to-root --reverse "$@"
    return $?
}
