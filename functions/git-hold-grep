# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-hold-grep ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-hold-grep||find a string in all entries in the git hold stash
    grep "$@" $(git-hold-find)
}
