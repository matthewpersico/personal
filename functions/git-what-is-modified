# -*- sh -*-

# <Class: git>
# <Function Justification: None>

git-what-is-modified ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-modified||

    if [ -n "$1" ]
    then
        echo 'Files altered: staged and unstaged'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --staged --unstaged
    fi
}
