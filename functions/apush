# -*- sh -*-

# <Function Class: bash arrays>
# <Function Justification: env mod>

apush ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||apush||Append arg2, arg3, etc.. to the end of the array named in arg1.

    local array_name="$1";shift
    asplice $array_name $(acount $array_name) 0 "$@"
    return $?
}
