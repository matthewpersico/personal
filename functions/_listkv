# -*- sh -*-

# <Function Class: bashhash>
# <Function Justification: env access>

_listkv ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit

    declare -n array_alias="$1"
    for i in "${!array_alias[@]}"
    do
        printf "%s %s\n" "$i" "${array_alias[$i]}"
    done
    return $(true)
}
