# -*- sh -*-

# <Function Class: util>

is_func_defined ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||is_func_defined||Is arg1 defined as a function. Returns 0 for no, 1 for yes.
    if [ -n "$(type $1 2>/dev/null | head -1 | grep 'is a function')" ]
    then
        return 0 ## UNIX TRUE
    else
        return 1 ## UNIX FALSE
    fi
}


