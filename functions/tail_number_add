# -*- sh -*-
# shellcheck shell=bash
# <Function Class: util>
# <Function Justification: none>

tail_number_add ()
{
    ##@@ none||none||tail_number_add||Add a '(' and an index number to each argument.
    local i=0
    for a in "$@"
    do
        ((i+=1))
        printf '%s (%d\n' "$a" "$i"
    done
    return $?
}
