# -*- sh -*-

# <Function Class: util>
# <Function Justification: maybe - frequency>

pause ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||pause||Print arguments as a prompt. Any keypress moves on.

    read -p "$*"
    return $?
}
