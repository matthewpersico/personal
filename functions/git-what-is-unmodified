# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-what-is-unmodified ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-unmodified||

    if [ -n "$1" ]
    then
        echo 'Files unaltered, but tracked'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --unmodified
    fi
}
