# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-hold-clean ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-hold-clean||remove all entries in the git hold stash
    rm -rf $(git-root)/.githold
}
