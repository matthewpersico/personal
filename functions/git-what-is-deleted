# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-what-is-deleted ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-what-is-deleted||

    if [ -n "$1" ]
    then
        echo 'Files deleted, both staged and not staged for commit'
    else
        $DEBUG_GIT_STATUS_PARSE $(which git-status-parse) --deleted
    fi
}
