# -*- sh -*-

# <Function Class: git>
# <Function Justification: env mod>

_git-cd-return ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    local tgt=$(apop GITFUNCS_OLDPWDS) ## We pop no matter what so bad dirs are
                                       ## cleared from the stack and we don't
                                       ## get stuck on them.
    [ -n "$tgt" ] && builtin cd $tgt
    return $?
}
