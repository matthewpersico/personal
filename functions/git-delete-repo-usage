# -*- sh -*-

# <Function Class: git

git-delete-repo-usage ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    local cmd=$1;shift
    local protocol=$1
    local protocolarg=$1
    if [ -z "$protocol" ]
    then
        protocol='protocol'
        protocolarg='protocol'
    else
        protocolarg=''
    fi

    cat <<EOUSAGE
Usage: $cmd $protocolarg namespace repo
       Whacks local repo $gitfuncs_gitroot/$protocol/namespace/repo.
       Asks about corresponding remote.
EOUSAGE
    return $OK
}


