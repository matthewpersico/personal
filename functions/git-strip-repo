# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-strip-repo ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-strip-repo||Remove the text '(repo)' from the argument.

    echo $1 | sed 's/(repo)//'
}
