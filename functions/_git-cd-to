# -*- sh -*-

# <Function Class: git>

_git-cd-to ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    local tgt=$(realpath $1)
    local src=$(pwd)
    builtin cd $tgt && apush GITFUNCS_OLDPWDS "$src" ## Push only if we can get
                                                     ## to the target
    return $?

}


