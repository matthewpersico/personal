# -*- sh -*-

# <Function Class: git

git-pull-usage ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    cat <<EOUSAGE
Usage: git-pull [-t]
       Does a git pull, followed by a descriptive git status.
       -t resets the timestamps on the pulled files
          to their original commit times
EOUSAGE
}


