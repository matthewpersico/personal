# -*- sh -*-

# <Function Class: util>

is-neg ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ none||none||is-neg||Is an integer and is < 0. Return 0 if true.

    is-int $1 && (( $1 < 0 )) && return $(true)
    return $(false)
}


