# -*- sh -*-

# <Function Class: git

git-checkout-last-existed ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-checkout-last-existed||Find the revision in which the given file last existed and check it out

    git checkout $(git-find-last-existed "$1") "$1"
}


