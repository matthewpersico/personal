# -*- sh -*-

# <Function Class: util>
# <Function Justification: none>

comment-stream ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${HOME}/personal/data/funcsaudit ## This is audit

    local comment="$1";shift
    IFS="$(printf '\n')"

    while read -r line
    do
        echo "${comment}$line"
    done
    return $(true)
}
