# -*- sh -*-

# <Function Class: git>
# <Function Justification: none>

git-push-hook ()
{
    echo "${FUNCNAME[0]} \"$@\" ## $(date +%Y%m%d%H%M%S)" >> ${TILDAE:-$HOME}/personal/data/funcsaudit ## This is audit
    ##@@ 0001||group||git-push-hook||

    git-hook-pushpull-guts $(_git-go-get 0 0 1 0 0 | grep -v ".git_template" )
}
